{
  "name": "Zoder.",
  "semanticHighlighting": true,
  "type": "dark",
  "colors": {
    "editor.background": "#2D2A2E",
    "editor.foreground": "#FCFCFA",
    "activityBar.background": "#211F22",
    "sideBar.background": "#2A272B",
    "statusBar.background": "#211F22",
    "statusBar.foreground": "#FCFCFA",
    "editor.selectionBackground": "#49483E",
    "editor.lineHighlightBackground": "#3E3D32",
    "editorCursor.foreground": "#FFCC00",
    "scrollbarSlider.background": "#4B4B4B",
    "scrollbarSlider.activeBackground": "#767676"
  },
  "tokenColors": [
    {
      "scope": ["keyword", "storage"],
      "settings": {
        "foreground": "#FF6188",
        "fontStyle": "italic"
      }
    },
    {
      "scope": ["string"],
      "settings": {
        "foreground": "#A9DC76"
      }
    },
    {
      "scope": ["function", "entity.name.function", "support.function"],
      "settings": {
        "foreground": "#FFD866"
      }
    },
    {
      "scope": ["variable", "variable.parameter"],
      "settings": {
        "foreground": "#FD9353",
        "fontStyle": "italic"
      }
    },
    {
      "scope": ["constant", "constant.numeric", "constant.language"],
      "settings": {
        "foreground": "#AB9DF2"
      }
    },
    {
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "foreground": "#5C6370"
      }
    },
    {
      "scope": ["operator", "keyword.operator"],
      "settings": {
        "foreground": "#FCFCFA",
        "fontStyle": "bold"
      }
    },
    {
      "scope": ["number", "constant.numeric"],
      "settings": {
        "foreground": "#FF6188"
      }
    },
    {
      "scope": ["type", "class", "support.class"],
      "settings": {
        "foreground": "#AB9DF2"
      }
    },
    {
      "scope": ["builtin", "support.function.builtin"],
      "settings": {
        "foreground": "#e8e678"
      }
    },
    {
      "scope": ["entity.name.tag", "support.class.component"],
      "settings": {
        "foreground": "#FF6188"
      }
    },
    {
      "scope": ["entity.other.attribute-name"],
      "settings": {
        "foreground": "#fffd93",
        "fontStyle": "italic"
      }
    },
    {
      "scope": [
        "entity.other.attribute-name.html",
        "entity.other.attribute-name.jsx"
      ],
      "settings": {
        "foreground": "#A9DC76"
      }
    },
    {
      "scope": [
        "string.quoted.double.html",
        "string.quoted.single.html",
        "string.quoted.double.jsx",
        "string.quoted.single.jsx"
      ],
      "settings": {
        "foreground": "#FFD866"
      }
    },
    {
      "scope": ["support.class.builtin", "support.type.primitive"],
      "settings": {
        "foreground": "#78DCE8"
      }
    },
    {
      "scope": ["meta.decorator.python"],
      "settings": {
        "foreground": "#FFD866"
      }
    },
    {
      "scope": ["keyword.control.import", "keyword.control.export"],
      "settings": {
        "foreground": "#FF6188",
        "fontStyle": "italic bold"
      }
    },
    {
      "scope": ["meta.type.annotation"],
      "settings": {
        "foreground": "#AB9DF2"
      }
    },
    {
      "scope": ["support.type.property-name.json"],
      "settings": {
        "foreground": "#FFD866" // Yellow color for JSON keys
      }
    },
    {
      "scope": [
        "meta.structure.dictionary.value.json string.quoted.double.json"
      ],
      "settings": {
        "foreground": "#A9DC76" // Green color for JSON values
      }
    },
    {
      "scope": ["keyword.operator.arithmetic.python"],
      "settings": {
        "foreground": "#FFD866" // Python arithmetic operators
      }
    },
    {
      "scope": [
        "keyword.control.import.python",
        "keyword.control.import.java",
        "keyword.control.import.ts",
        "keyword.control.import.js"
      ],
      "settings": {
        "foreground": "#FF6188", // Import statements
        "fontStyle": "italic"
      }
    },
    {
      "scope": [
        "support.function.python",
        "support.function.js",
        "support.function.ts"
      ],
      "settings": {
        "foreground": "#FFD866" // Function names in Python, JavaScript, TypeScript
      }
    },
    {
      "scope": [
        "entity.name.class.python",
        "entity.name.class.java",
        "entity.name.class.ts",
        "entity.name.class.js"
      ],
      "settings": {
        "foreground": "#AB9DF2" // Class names in Python, JavaScript, Java, TypeScript
      }
    },
    {
      "scope": ["storage.type.cs", "storage.type.cpp", "storage.type.rust"],
      "settings": {
        "foreground": "#FF6188", // Keywords in C#, C++, Rust
        "fontStyle": "italic"
      }
    },
    {
      "scope": [
        "variable.other.constant.swift",
        "variable.other.constant.kotlin"
      ],
      "settings": {
        "foreground": "#AB9DF2" // Constants in Swift, Kotlin
      }
    },
    {
      "scope": ["support.class.php", "support.class.ruby"],
      "settings": {
        "foreground": "#FFD866" // Support classes in PHP, Ruby
      }
    },
    {
      "scope": ["constant.language.sql", "keyword.other.sql"],
      "settings": {
        "foreground": "#FF6188" // SQL constants and keywords
      }
    },
    {
      "scope": ["support.function.matlab"],
      "settings": {
        "foreground": "#FFD866" // MATLAB functions
      }
    },
    {
      "scope": ["support.type.scala", "support.type.dart"],
      "settings": {
        "foreground": "#AB9DF2" // Types in Scala, Dart
      }
    },
    {
      "scope": ["constant.language.perl"],
      "settings": {
        "foreground": "#FF6188" // Constants in Perl
      }
    },
    {
      "scope": ["support.function.builtin.shell"],
      "settings": {
        "foreground": "#FFD866" // Built-in functions in Shell
      }
    },
    {
      "scope": ["keyword.control.lua"],
      "settings": {
        "foreground": "#FF6188" // Lua keywords
      }
    },
    {
      "scope": ["entity.name.function.oc"],
      "settings": {
        "foreground": "#FFD866" // Objective-C function names
      }
    },
    {
      "scope": ["entity.name.tag.haskell"],
      "settings": {
        "foreground": "#AB9DF2" // Tags in Haskell
      }
    },
    {
      "scope": ["support.function.erlang"],
      "settings": {
        "foreground": "#FFD866" // Functions in Erlang
      }
    },
    {
      "scope": ["keyword.control.elixir"],
      "settings": {
        "foreground": "#FF6188" // Keywords in Elixir
      }
    },
    {
      "scope": ["storage.type.fsharp"],
      "settings": {
        "foreground": "#AB9DF2" // F# types
      }
    },
    {
      "scope": ["entity.name.tag.groovy"],
      "settings": {
        "foreground": "#FFD866" // Tags in Groovy
      }
    },
    {
      "scope": ["support.function.builtin.julia"],
      "settings": {
        "foreground": "#FFD866" // Julia built-in functions
      }
    },
    {
      "scope": ["keyword.control.solidity"],
      "settings": {
        "foreground": "#FF6188" // Solidity keywords
      }
    },
    {
      "scope": ["storage.type.vhdl", "storage.type.verilog"],
      "settings": {
        "foreground": "#FFD866" // VHDL/Verilog types
      }
    },
    {
      "scope": ["keyword.other.assembly"],
      "settings": {
        "foreground": "#FF6188" // Assembly keywords
      }
    }
  ]
}

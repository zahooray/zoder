{
  "name": "Zoder.",
  "type": "dark",
  "colors": {
    "editor.background": "#2D2A2E",
    "editor.foreground": "#FCFCFA",
    "activityBar.background": "#211F22",
    "sideBar.background": "#2A272B",
    "statusBar.background": "#211F22",
    "statusBar.foreground": "#FCFCFA",
    "editor.selectionBackground": "#49483E",
    "editor.lineHighlightBackground": "#3E3D32",
    "editorCursor.foreground": "#FFCC00",
    "scrollbarSlider.background": "#4B4B4B",
    "scrollbarSlider.activeBackground": "#767676"
  },
  "tokenColors": [
    {
      "scope": ["keyword", "storage"],
      "settings": {
        "foreground": "#FF6188",
        "fontStyle": "italic"
      }
    },
    {
      "scope": ["string"],
      "settings": {
        "foreground": "#A9DC76"
      }
    },
    {
      "scope": ["function", "entity.name.function", "support.function"],
      "settings": {
        "foreground": "#FFD866"
      }
    },
    {
      "scope": ["variable", "variable.parameter"],
      "settings": {
        "foreground": "#FD9353",
        "fontStyle": "italic"
      }
    },
    {
      "scope": ["constant", "constant.numeric", "constant.language"],
      "settings": {
        "foreground": "#AB9DF2"
      }
    },
    {
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "foreground": "#5C6370"
      }
    },
    {
      "scope": ["operator", "keyword.operator"],
      "settings": {
        "foreground": "#FCFCFA",
        "fontStyle": "" // Removed italic
      }
    },
    {
      "scope": ["number", "constant.numeric"],
      "settings": {
        "foreground": "#FF6188"
      }
    },
    {
      "scope": ["type", "class", "support.class"],
      "settings": {
        "foreground": "#AB9DF2"
      }
    },
    {
      "scope": ["builtin", "support.function.builtin"],
      "settings": {
        "foreground": "#e8e678"
      }
    },
    {
      "scope": ["entity.name.tag", "support.class.component"],
      "settings": {
        "foreground": "#FF6188"
      }
    },
    {
      "scope": ["entity.other.attribute-name"],
      "settings": {
        "foreground": "#fffd93",
        "fontStyle": "italic"
      }
    },
    {
      "scope": [
        "entity.other.attribute-name.html",
        "entity.other.attribute-name.jsx"
      ],
      "settings": {
        "foreground": "#A9DC76"
      }
    },
    {
      "scope": [
        "string.quoted.double.html",
        "string.quoted.single.html",
        "string.quoted.double.jsx",
        "string.quoted.single.jsx"
      ],
      "settings": {
        "foreground": "#FFD866"
      }
    },
    {
      "scope": ["support.class.builtin", "support.type.primitive"],
      "settings": {
        "foreground": "#78DCE8"
      }
    },
    {
      "scope": ["meta.decorator.python"],
      "settings": {
        "foreground": "#FFD866"
      }
    },
    {
      "scope": ["keyword.control.import", "keyword.control.export"],
      "settings": {
        "foreground": "#FF6188",
        "fontStyle": "italic bold"
      }
    },
    {
      "scope": ["meta.type.annotation"],
      "settings": {
        "foreground": "#AB9DF2"
      }
    },
    {
      "scope": ["support.type.property-name.json"],
      "settings": {
        "foreground": "#FFD866" // Yellow color for JSON keys
      }
    },
    {
      "scope": [
        "meta.structure.dictionary.value.json string.quoted.double.json"
      ],
      "settings": {
        "foreground": "#A9DC76" // Green color for JSON values
      }
    }
  ]
}
